{
  "name": "online-clipboard-api",
  "version": "1.0.0",
  "author": "17ban",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js --http 8010",
    "nodemon": "nodemon ./bin/www.js --http 8010",
    "bg": "nohup nodemon ./bin/www.js --http 8010 &"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "nodemon": "^2.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nodemonConfig": {
    "ext": "js",
    "delay": "2500"
  }
}
